project Stm32 is

   for Languages use ("Ada");
   for Source_Dirs use ("src");
   for Object_Dir use "obj";

   package Compiler is
      for Default_Switches ("ada") use ("-g", "-gnatwa", "-gnatQ", "-gnatw.X", "-O0");
   end Compiler;

   package Builder is
      for Default_Switches ("ada") use ("-g");
   end Builder;

end Stm32;
